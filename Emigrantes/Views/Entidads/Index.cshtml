        <td>
            @Html.DisplayFor(modelItem => item.IdTipoSolicitud)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sectorEntidad.Sector)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tipoSolicitud.Solicitud)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
