        <th>
            @Html.DisplayNameFor(model => model.Ciudad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdTipoSolicitud)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sectorEntidad.Sector)
        </th>
